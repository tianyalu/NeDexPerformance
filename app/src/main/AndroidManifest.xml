<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sty.ne.dexperformance">

<!--    android:name="com.sty.ne.proxy_core.ProxyApplication"-->
    <application
        android:name="com.sty.ne.proxy_core.ProxyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <!--真实的application全称-->
        <meta-data android:name="app_name" android:value="com.sty.ne.dexperformance.app.MyApplication"/>
        <!--用来存放dex文件的目录-->
        <meta-data android:name="app_version" android:value="\dexDir_1.0"/>
    </application>

</manifest>